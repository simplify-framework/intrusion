{
  "name": "simplify-intrusion",
  "version": "0.1.3",
  "description": "Simplify Intrusion Detection/Prevention for NodeJS",
  "main": "index.js",
  "bin": {
    "simplify-intrusion": "index.js"
  },
  "scripts": {
    "test": "npm run test-1 && npm run test-2 && npm run test-3 && npm run test-4 && npm run test-lambda",
    "test-0": "npm run test-commonjs 0-default",
    "test-1": "npm run test-commonjs 1-block-all",
    "test-2": "npm run test-commonjs 2-allow-hash",
    "test-3": "npm run test-commonjs 3-allow-host",
    "test-4": "npm run test-commonjs 4-allow-all",
    "test-lambda": "node -e \"process.env.ORIGIN_LAMBDA_HANDLER=`${require('path').resolve('tests/lambda')}.handler`;require('./reflection').handler(null,null,function(err,data) {console.log(err?err:data)})\"",
    "test-commonjs": "node tests/commonjs/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/simplify-framework/intrusion.git"
  },
  "keywords": [
    "intrusion",
    "security",
    "firewall",
    "lambda"
  ],
  "author": "Duong Dinh Cuong",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/simplify-framework/intrusion/issues"
  },
  "homepage": "https://github.com/simplify-framework/intrusion#readme",
  "dependencies": {
    "aws-sdk": "^2.1060.0",
    "lodash": "^4.17.21"
  }
}